    ["sh" "-c" "cd %{prefix}%/xtensa-esp32-elf/ && rm -f toolchain-esp32-osx-*.env toolchain-esp32-osx-*.info toolchain-esp32-osx-*.err toolchain-esp32-osx-*.out"]
]
